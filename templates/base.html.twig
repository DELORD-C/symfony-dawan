{% block header %}
    <!DOCTYPE html>
    <html lang="en">
        <head>
            <meta charset="UTF-8">
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
            {{ encore_entry_link_tags('app') }}
            {% block title %}
                {% if titre is defined %}
                    <title>{{ titre }}</title>
                {% else %}
                    <title>Symfony</title>
                {% endif %}
            {% endblock %}
        </head>
        <body>
{% endblock %}
{% block content %}
    {% if content is defined %}
        {{ content }}
    {% endif %}
{% endblock %}
{% block footer %}
        {{ encore_entry_script_tags('app') }}
        </body>
</html>
{% endblock %}